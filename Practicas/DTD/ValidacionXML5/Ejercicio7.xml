<!-- Declaración DTD -->
<!DOCTYPE colores [
	<!ELEMENT colores (color*)>
	<!ELEMENT color (nombreSvg, codigo)>
	<!ELEMENT nombreSvg (#PCDATA)>
	<!ELEMENT codigo (rgb|cmyk)>
	<!ELEMENT rgb (#PCDATA)>
	<!ELEMENT cmyk (#PCDATA)>
]>
<!-- Listado de colores -->
<colores>
	<!-- Color individual -->
	<color>
		<!-- Descripcion del color -->
		<nombreSvg>Purple</nombreSvg>
		<codigo>
			<rgb>#800080</rgb>
		</codigo>
	</color>
	<color>
		<nombreSvg>Purple</nombreSvg>
		<codigo>
			<cmyk>#00FF007F</cmyk>
		</codigo>
	</color>
</colores>