<!-- Declaración DTD -->
<!DOCTYPE cuadros [
	<!ELEMENT cuadros (cuadro*)>
	<!ELEMENT cuadro EMPTY>
	<!ATTLIST cuadro
		titulo CDATA #REQUIRED
		autor CDATA #REQUIRED
    >
]>
<!-- Contenedor de cuadros -->
<cuadros>
	<!-- Cuadro individual con atributos describiendole -->
	<cuadro titulo="Adán y Eva" autor="Alberto Durero" />
	<cuadro autor="Lucas Cranach, el viejo" titulo="Adán y Eva" />
</cuadros>